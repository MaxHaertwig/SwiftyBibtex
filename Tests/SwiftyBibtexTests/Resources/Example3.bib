    classes = {Type of Research{Applied{Modelled Artifact{code}, Model type{structure}, Purpose{assurance}, Techniques{analysis}}, Fundamental{none}}, Application Domains{travel}, Research areas{formal methods}, Initiative{none}, Venue{seams}, Venue Type{c}},
