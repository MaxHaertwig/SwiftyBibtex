    domain = {ecommerce},
    initiatives = {none},
    keywords = {Bayes methods;belief networks;diagnostic expert systems;fault diagnosis;statistical
        analysis;unsupervised learning;Bayesian network;TPC-W industry-standard
        e-commerce benchmark;anomaly diagnosis;automatic fault injection;belief
        network;expert knowledge;generic statistical models;resource-specific
        models;semantic-driven model composition;semantic-driven modeling;system
        administrator;system modeling;Analytical models;Bayesian methods;Computer
        industry;Databases;Industrial relations;Large-scale systems;Modeling;Prototypes;Vehicle
        dynamics;Web server},
