@INPROCEEDINGS{Evesti2010,
    author = {Evesti, A. and Ovaska, E.},
    title = {Ontology-Based Security Adaptation at Run-Time},
    booktitle = {Self-Adaptive and Self-Organizing Systems (SASO), 2010 4th IEEE International
        Conference on},
    year = {2010},
    pages = {204-212},
    month = "Sept",
    abstract = {This paper describes how software is able to autonomously adapt its
        security mechanisms based on knowledge from security ontology. Security
        adaptation is required because a software's environment changes during
        run-time. Thus, all security requirements cannot be defined beforehand.
        To achieve security adaptation, we have combined a security ontology
        that defines security mechanisms, security objectives, and high level
        security measurements. The run-time security adaptation utilises
        this security ontology to adapt security mechanisms or their parameters
        to fulfil security requirements for each environment and usage situation.
        The novelty of this approach comes from the utilisation of ontologies
        and security measurements, which makes adaptation flexible. We validate
        our security adaptation with a case study in a smart space environment.
        The case study proves that security adaptation is able to work autonomously
        without other user actions.},
